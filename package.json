{
  "name": "vue2-city-selector",
  "version": "0.1.5",
  "description": "A Vue.js selector to city",
  "author": "jinghaoo <jingh1024@163.com>",
  "private": false,
  "main": "dist/vue2-city-selector.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:jinghaoo/city-selector.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jinghaoo/city-selector/issues"
  },
  "keywords": [
    "vue",
    "city",
    "selector",
    "vue2-city-selector"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vue2-city-selector ./src/index.js",
    "build:wc": "vue-cli-service build --target wc --name vue2-city-selector ./src/vue2-city-selector.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
